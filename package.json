{
  "name": "serverless-duplocloud",
  "version": "0.0.1",
  "description": "A plugin and provider for the serverless framework",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duplocloud/servereless.git"
  },
  "keywords": [
    "serverless"
  ],
  "author": "kelly@duplocloud.net",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duplocloud/servereless/issues"
  },
  "homepage": "https://github.com/duplocloud/servereless#readme",
  "scripts": {
    "test": "echo 'No Tests yet'",
    "mocha": "NODE_PATH=src/ TS_NODE_FILES=true nyc mocha --config .mocharc.json $*",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "debug": "node --enable-source-maps -r ts-node/register --inspect $(which serverless) print --config examples/simple/serverless.yml --stage dev01",
    "lint": "eslint --quiet 'src/**/*.ts'",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "build": "NODE_PATH=src/ TS_NODE_FILES=true tsc",
    "build:prod": "TS_NODE_FILES=true tsc -p tsconfig.prod.json",
    "clean": "rm -rf node_modules dist public .nyc_output",
    "example": "sls print --config examples/simple/serverless.yml --stage dev01"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/serverless": "^3.12.24",
    "eslint": "^9.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "peerDependencies": {
    "serverless": "^4.4.14"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
